<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ParallaxImageView">
		<attr name="parallaxSpeed" format="integer" />
		<attr name="allowOutside" format="boolean" />
		<attr name="parallaxDirection" format="enum" >
			<enum name="horizontal" value="0x01" />
			<enum name="vertical" value="0x10" />
			<enum name="both" value="0x11" />
		</attr>
	</declare-styleable>

	<declare-styleable name="ParallaxLayout">
		<attr name="parallaxBackgroundSpeed" format="integer" />
		<attr name="parallaxBackgroundDirection" format="enum" >
			<enum name="horizontal" value="0x01" />
			<enum name="vertical" value="0x10" />
			<enum name="both" value="0x11" />
		</attr>
	</declare-styleable>
</resources>